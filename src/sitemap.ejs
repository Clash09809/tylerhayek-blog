---
permalink: /sitemap.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <% collections.all.forEach(item => { %>
    <url>
      <loc>https://prodigy-gaming.com<%=item.url%></loc>
      <lastmod><%= item.date.toISOString() %></lastmod>
      <changefreq><%= item.data.changefreq %></changefreq>
      <priority><%= item.data.priority %></priority>
    </url>
  <% }) %>

  <% articles.forEach(article => { %>
    <url>
      <loc>https://prodigy-gaming.com/blog/show/<%=article.slug%></loc>
      <lastmod><%= article.updated %></lastmod>
      <changefreq>monthly</changefreq>
      <priority>0.9</priority>
    </url>
  <% }) %>
</urlset>

